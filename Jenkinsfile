pipeline {
    agent any

    stages {
        stage('Clone Repo') {
            steps {
                echo 'Repo berhasil dikloning oleh Jenkins.'
            }
        }

        stage('Build') {
            steps {
                echo 'Tahap build berjalan.'
            }
        }

        stage('Test') {
            steps {
                echo 'Tes unit berjalan (simulasi).'
            }
        }
    }
}
